<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/DMN/20151101/dmn.xsd" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/1.0" id="Definitions_1leta2v" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="3.7.0">
  <decision id="Decision_0zjwmt2" name="Risk Rating">
    <extensionElements>
      <biodi:bounds x="157" y="81" width="180" height="80" />
    </extensionElements>
    <decisionTable id="decisionTable_1">
      <input id="input_1" label="ApplicantAge">
        <inputExpression id="inputExpression_1" typeRef="string">
          <text>The applicant's age</text>
        </inputExpression>
      </input>
      <input id="InputClause_1u5ws9s" label="MedicalHistory">
        <inputExpression id="LiteralExpression_0eegvtl" typeRef="string">
          <text>The applicant's medical history considering serious illnesses</text>
        </inputExpression>
        <inputValues id="UnaryTests_14vc5rc">
          <text>"good","bad"</text>
        </inputValues>
      </input>
      <output id="output_1" label="Scoring" typeRef="string">
        <outputValues id="UnaryTests_0el1h3r">
          <text>"low","medium","high","very high","very low"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_02z3n4d">
        <description>"Very low healthcare risks -&gt; very low insurance fee"</description>
        <inputEntry id="UnaryTests_16p8m3a">
          <text>&lt;25</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1yqve18">
          <text>"good"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0ao9d8u">
          <text>"low"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0baqmdi">
        <description>"Low healthcare risks -&gt; low insurance fee"</description>
        <inputEntry id="UnaryTests_0l6rfv9">
          <text>&lt;25</text>
        </inputEntry>
        <inputEntry id="UnaryTests_020bqjz">
          <text>"bad"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1xsx8xv">
          <text>"medium"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_09u175b">
        <description>"Medium healthcare risks -&gt; moderate insurance fee"</description>
        <inputEntry id="UnaryTests_1sogwcc">
          <text>[25...60]</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1pcikhp">
          <text>"good"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_05xzipo">
          <text>"medium"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_12tnsrb">
        <description>"Medium healthcare risks -&gt; moderate insurance fee"</description>
        <inputEntry id="UnaryTests_08ipi6c">
          <text>&gt;60</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0jtrawy">
          <text>"good"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1aw62vl">
          <text>"medium"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1dz5ore">
        <description>"High healthcare risks -&gt; high insurance fee"</description>
        <inputEntry id="UnaryTests_1tpebxe">
          <text>&gt;60</text>
        </inputEntry>
        <inputEntry id="UnaryTests_15f4qzg">
          <text>"bad"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0m8je4f">
          <text>"high"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
